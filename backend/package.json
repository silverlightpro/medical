    "express-rate-limit": "7.1.5",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.1",
    "pdf-parse": "1.1.1",
    "xss": "1.0.15",
    "zod": "3.23.8",
    "nodemailer": "6.9.13"
  },
  "devDependencies": {
    "@babel/cli": "7.24.7",
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "nodemon": "3.1.4",
    "prisma": "5.16.2",
    "rimraf": "5.0.7",
    "typescript": "5.5.4"
